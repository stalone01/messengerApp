{% extends 'base.html.twig' %}

{% block title %}Hello ChatController!{% endblock %}

{% block body %}
<div>
    {% for user in users %}
        <li>{{ user.username }}</li>
    {% endfor %}
</div>
<div id="app">
    <user-list :users="{{ users|json_encode|raw }}"></user-list>
</div>

{% endblock %}
